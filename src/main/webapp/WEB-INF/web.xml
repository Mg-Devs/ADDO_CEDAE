<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <welcome-file-list>
        <welcome-file>index</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>index</servlet-name>
        <servlet-class>webpages.index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>agendarCita</servlet-name>
        <servlet-class>webpages.agendarCita</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>webpages.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>medico</servlet-name>
        <servlet-class>webpages.medico</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>farmacia</servlet-name>
        <servlet-class>webpages.farmacia</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>recepcionista</servlet-name>
        <servlet-class>webpages.recepcionista</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PDFReceta</servlet-name>
        <servlet-class>webpages.PDFReceta</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>agendarCita</servlet-name>
        <url-pattern>/agendarCita</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>medico</servlet-name>
        <url-pattern>/medico</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>farmacia</servlet-name>
        <url-pattern>/farmacia</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>recepcionista</servlet-name>
        <url-pattern>/recepcionista</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PDFReceta</servlet-name>
        <url-pattern>/PDF/Receta</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
